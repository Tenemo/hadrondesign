{
    "name": "hadrondesign",
    "version": "1.3.0",
    "description": "",
    "scripts": {
        "start-message": "babel-node config/startMessage.js",
        "prestart": "rimraf node_modules/.cache && npm run start-message",
        "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\"",
        "open:src": "babel-node config/srcServer.js",
        "open:dist": "babel-node config/distServer.js",
        "lint": "esw src config --ext js,jsx",
        "lint:watch": "npm run lint -- --watch",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "prebuild": "npm run lint && npm run test && npm run clean-dist",
        "build": "babel-node config/build.js && npm run open:dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "npm run test -- --coverage "
    },
    "repository": {
        "type": "git",
        "url": "git+https://tenemo@github.com/tenemo/hadrondesign.git"
    },
    "author": "tenemo@gmail.com",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/tenemo/hadrondesign/issues"
    },
    "homepage": "https://github.com/tenemo/hadrondesign#readme",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.1.1",
        "classnames": "^2.2.6",
        "history": "^4.7.2",
        "jquery": "^3.3.1",
        "node-fetch": "^2.1.2",
        "object-assign": "^4.1.1",
        "popper.js": "^1.14.3",
        "prop-types": "^15.6.1",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^5.0.0-alpha.9",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "toastr": "^2.1.4"
    },
    "devDependencies": {
        "autoprefixer": "^8.6.2",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-react-jsx-source": "^6.22.0",
        "babel-plugin-transform-react-pug": "^6.0.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "browser-sync": "^2.24.4",
        "chalk": "^2.4.1",
        "concurrently": "^3.5.1",
        "connect-history-api-fallback": "^1.5.0",
        "css-loader": "^0.28.11",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^4.19.1",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jest": "^21.17.0",
        "eslint-plugin-react": "^7.9.1",
        "eslint-plugin-react-pug": "^0.3.1",
        "eslint-watch": "^3.1.5",
        "eventsource-polyfill": "^0.9.6",
        "file-loader": "^1.1.11",
        "hard-source-webpack-plugin": "^0.7.5",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.1.0",
        "jest-cli": "^23.1.0",
        "mini-css-extract-plugin": "^0.4.0",
        "node-sass": "^4.9.0",
        "postcss-loader": "^2.1.5",
        "pug": "^2.0.3",
        "pug-loader": "^2.4.0",
        "react-hot-loader": "^4.3.1",
        "redux-devtools-extension": "^2.13.2",
        "redux-immutable-state-invariant": "^2.1.0",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.12.0",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-hot-middleware": "^2.22.2",
        "webpack-md5-hash": "0.0.6"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/config/enzymeSetup.js",
        "moduleNameMapper": {
            "^.+\\.(css|scss)$": "identity-obj-proxy"
        },
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}
